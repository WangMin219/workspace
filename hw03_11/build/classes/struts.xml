<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
     <constant name="struts.devMode" value="true" />

  <package name="default" namespace="/" extends="struts-default">
  <action name="addBookAction" class="com.action.BookAction" method="addBookMethod">
   <result name="addOK" type="chain">getBookAction</result>
    </action>
         <action name="getBookAction" class="com.action.BookAction" method="getBookMethod">
             <result name="getOK">/showBookInfo.jsp</result>
         </action>
                  <action name="findBookAction" class="com.action.BookAction" method="findBookMethod">
             <result name="findOK">/showOneBook.jsp</result>
         </action>
         <action name="updateBookAction" class="com.action.BookAction" method="updateBookMethod">
              <result name="updateOK">/showDeleteBookNewInfo.jsp</result>
         </action> 
          <action name="deleteBookAction" class="com.action.BookAction" method="delectBookMethod">
            <result name="deleteOK">/showDeleteBookNewInfo.jsp</result>
        </action>
   
  
  
  
  
  </package>
  


</struts>
